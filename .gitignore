# --- RegexPlay .gitignore ---
# Node dependencies
/node_modules/

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage
coverage/
*.lcov

# Build artifacts (if added later)
dist/
build/
.tmp/
.temp/

# Cache
.npm/
.eslintcache
.cache/

# TypeScript (future-proofing)
*.tsbuildinfo

# Environment variables
.env
.env.local
.env.*.local

# OS / Editor junk
.DS_Store
Thumbs.db
.idea/
.vscode/*
!.vscode/extensions.json
!.vscode/settings.json
*.swp

# Archives / packages
*.tgz

# Debug / profiling
*.stackdump

# Node REPL history
.node_repl_history

# npm versioning
package-lock.json

# Yarn / pnpm locks (if not used)
yarn.lock
pnpm-lock.yaml

# Sessions generated by the app
.regexplay-sessions/

# Generated docs (if any in future)
/docs/.cache/

# Coverage tooling
.nyc_output/

# GitHub metadata noise
.turbolift/

# Dependency vulnerability audits
audit.json

# Temporary scratch files
scratch.*
*.tmp

# macOS metadata
.AppleDouble
.LSOverride

# End of file
